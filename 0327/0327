[2]
*******************************************************
#include<stdio.h>
 
int main()
{
    int i,j,k;
    printf("\n");
    for(i=1;i<5;i++) {          // i初始值為1;判斷i<5:遞增
        for(j=1;j<5;j++) {      //j初始值為1;判斷j<5:遞增
            for (k=1;k<5;k++) { // 確保i、j、k三位互不相同
                if (i!=k&&i!=j&&j!=k) { 
                    printf("%d,%d,%d\n",i,j,k);  //列印出i,j,k的值
                }
            }
        }
    }
}
//main() 這個函式就是你程式裡的主函式
//printf 列印
//for(;;) 迴圈
*******************************************************
#include <iostream>            //叫編譯器去呼叫位於iostream當中標準資料流I/O程式庫宣告式
 using namespace std;
int main()
{
    int i,j,k;
    std::cout << std::endl;     //換行
    for(i=1;i<5;i++) {          // i初始值為1;判斷i<5:遞增
        for(j=1;j<5;j++) {      // 確保i、j、k三位互不相同
            for (k=1;k<5;k++) { // 確保i、j、k三位互不相同
                if (i!=k&&i!=j&&j!=k) {  
                    cout<<""<<i;        
                    cout<<","<<j;
                    cout<<","<<k<<endl;  //列印出i,j,k的值
                }
            }
        }
    }
}
*******************************************************
